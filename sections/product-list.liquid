{%render 'product-list' heading:section.setting.heading, collection_handle:collection%}


{% schema %}
{
    "name": "Product List",
    "tag": "section",
    "class": "product-list",
    "settings": [
    {
        "type": "text",
        "id": "heading",
        "label": "Label",
        "default": "Product List"
    },
    {
        "type": "collection",
        "id": "collection",
        "label": "Collection"
    }
    ]
}
{% endschema %}
